{
  "extends": ["@atolye15/eslint-config"],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "settings": {
    "import/extensions": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx", ".svg"]
      }
    }
  }
}
